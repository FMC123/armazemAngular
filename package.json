{
  "name": "agrotopus-wms",
  "version": "0.0.1",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --dev --e=dev --port=3000",
    "start:dev": "ng serve --dev --e=dev --port=3000",
    "start:tomcat": "ng serve --dev --e=tomcat --port=3000",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build:profile": "ng build --prod --e=hom --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "build:spring": "npm run clean:webapp && ng build --prod --aot=false --e=spring && npm run publish:webapp:windows && npm run publish:webapp:mac",
    "build:tomcat": "npm run clean:webapp && ng build --prod --aot=false --e=tomcat --base-href /agrotopuswms/ && npm run publish:webapp:windows && npm run publish:webapp:mac",
    "build:hom": "npm run clean:webapp && ng build --prod --aot=false --e=hom --base-href /agrotopuswms/ && npm run publish:webapp:windows && npm run publish:webapp:mac",
    "rimraf": "rimraf",
    "clean:webapp": "npm run rimraf -- ../webapp",
    "publish:webapp:mac": "copyfiles 'dist/**/*' -u 1 ../webapp",
    "publish:webapp:windows": "copyfiles dist/**/* -u 1 ../webapp"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.3.1",
    "@angular/compiler": "2.3.1",
    "@angular/core": "2.3.1",
    "@angular/forms": "2.3.1",
    "@angular/http": "2.3.1",
    "@angular/platform-browser": "2.3.1",
    "@angular/platform-browser-dynamic": "2.3.1",
    "@angular/router": "3.3.1",
    "angular2-powerbi": "^0.1.1",
    "angular2-text-mask": "2.1.0",
    "assert": "1.4.1",
    "core-js": "2.4.1",
    "file-saver": "^1.3.3",
    "filesize": "3.3.0",
    "fuse.js": "2.5.0",
    "moment": "^2.22.2",
    "ng2-select": "1.2.0",
    "ngx-bootstrap": "^1.9.3",
    "ngx-cookie": "1.0.0",
    "numeral": "2.0.4",
    "rxjs": "5.0.1",
    "text-mask-addons": "0.7.2",
    "ts-helpers": "1.1.1",
    "uuid": "3.0.1",
    "verror": "1.9.0",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.2",
    "@angular/compiler-cli": "2.3.1",
    "@types/core-js": "0.9.28",
    "@types/jasmine": "2.5.38",
    "@types/jquery": "2.0.32",
    "@types/node": "6.0.42",
    "codelyzer": "2.0.0-beta.1",
    "copyfiles": "1.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "ngx-typeahead": "0.0.3",
    "prettier": "1.10.2",
    "protractor": "4.0.13",
    "ts-node": "1.2.1",
    "tslint": "4.0.2",
    "typescript": "2.0.3",
    "webpack-bundle-analyzer": "^2.9.1"
  }
}
